{
    "name": "ErgonoMission",
    "description": "Nossa missão é ajudar a gerenciar seu uso de computador!",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": [
        "storage",
        "alarms",
        "notifications",
        "tabs",
        "cookies",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "index.html"
    }
}